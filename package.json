{
  "name": "proxy-polyfill",
  "version": "0.2.0",
  "description": "Polyfill for the Proxy object",
  "main": "./lib/proxy.js",
  "module": "./es/proxy.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoogleChrome/proxy-polyfill.git"
  },
  "keywords": [
    "polyfill",
    "html5",
    "proxy",
    "es6"
  ],
  "author": "The Chromium Authors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GoogleChrome/proxy-polyfill/issues"
  },
  "homepage": "https://github.com/GoogleChrome/proxy-polyfill#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "cross-env": "^5.1.4",
    "mocha": "^3.5.3",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-bundle-size": "vimeo/rollup-plugin-bundle-size",
    "rollup-plugin-closure-compiler-js": "^1.0.6",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "scripts": {
    "build:es": "cross-env NODE_ENV=es rollup -c -o es/proxy.js",
    "build:cjs": "cross-env NODE_ENV=cjs rollup -c -o lib/proxy.js",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/proxy.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/proxy.min.js",
    "build": "npm run build:cjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "pretest": "npm run build:cjs",
    "test": "mocha"
  }
}
